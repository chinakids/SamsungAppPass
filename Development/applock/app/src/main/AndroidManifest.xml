<?xml version="1.0" encoding="utf-8"?>

<manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="chinakids.applock">

<application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:theme="@style/BaseTheme.MainTheme">

    <activity
        android:name="chinakids.applock.activities.MainActivity"
        android:label="@string/app_name"
        android:theme="@style/BaseTheme.MainTheme.WithNavDrawer">
        <intent-filter>
            <action android:name="android.intent.action.MAIN"/>

            <category android:name="android.intent.category.LAUNCHER"/>
        </intent-filter>
    </activity>

    <activity
        android:name="chinakids.applock.activities.OtherActivity"
        android:label="@string/other_activity"
        android:parentActivityName="chinakids.applock.activities.MainActivity">
    </activity>
</application>

</manifest>
